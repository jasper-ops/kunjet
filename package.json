{
  "name": "kunjet",
  "main": "src/index.js",
  "version": "0.0.4",
  "type": "module",
  "author": "Jasper <wx2080969361@gmail.com>",
  "description": "Some commonly used javascript utility functions",
  "keywords": [
    "utils",
    "format",
    "money",
    "phone"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasper-ops/utils"
  },
  "homepage": "https://github.com/jasper-ops/utils",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "tsup src/*.ts --watch src --clean",
    "build": "tsup src/*.ts --format cjs,esm --dts --clean",
    "commit": "git-cz",
    "release": "node ./scripts/release.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "bumpp": "^9.4.1",
    "husky": "^9.0.11",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1"
  },
  "packageManager": "pnpm@9.1.0+sha256.22e36fba7f4880ecf749a5ca128b8435da085ecd49575e7fb9e64d6bf4fad394",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
